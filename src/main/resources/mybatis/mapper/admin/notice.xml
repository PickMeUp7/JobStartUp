<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.pickmeup.jobstartup.admin.repository.NoticeRepository">
    <select id="selectNoticeList" resultType="com.pickmeup.jobstartup.admin.dto.NoticeDTO">
        <![CDATA[
        SELECT *
        FROM NOTICE
        ORDER BY NOT_REGDATE DESC
        ]]>
    </select>
</mapper>
