<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.pickmeup.jobstartup.resume.repository.ResumeRepository">
    <insert id="insertResumeLoc" parameterType="ResumeLocDTO">
        INSERT INTO RESUME_LOC (RESUME_NO, LOC_DETAIL_CODE_NUM)
        VALUES
        <foreach collection="resumeLocDTOList" item="resumeLocDTO" separator=",">
            (#{resumeLocDTO.resume_no}, #{resumeLocDTO.loc_detail_code_num})
        </foreach>
    </insert>

    <delete id="deleteResumeLoc" parameterType="int">
        <![CDATA[
        DELETE RESUME_LOC
        WHERE RESUME_NO=#{resume_no}
        ]]>
    </delete>
</mapper>